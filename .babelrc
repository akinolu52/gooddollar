{
    "presets": [
        "module:metro-react-native-babel-preset"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "@constants": "./src/constants",
                    "@components": "./src/components",
                    "@utils": "./src/utils",
                    "@screens": "./src/screens"
                }
            }
        ]
    ]
}